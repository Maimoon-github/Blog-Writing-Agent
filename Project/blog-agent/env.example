# Base URL for Ollama API
OLLAMA_BASE_URL=http://localhost:11434
# Model name for Ollama
OLLAMA_MODEL=mistral
# Base URL for SearXNG search instance
SEARXNG_BASE_URL=http://localhost:8080
# HuggingFace model ID for Stable Diffusion
SD_MODEL_ID=CompVis/stable-diffusion-v1-4
# Directory for ChromaDB persistence
CHROMA_PERSIST_DIR=./memory/chroma_data
# Directory for output files (blog posts, images)
OUTPUT_DIR=./outputs
# Directory for cache files
CACHE_DIR=./memory/cache
# Directory for logs
LOG_DIR=./outputs/logs
# Cache TTL in seconds (e.g., 86400 = 24 hours)
CACHE_TTL_SECONDS=86400
# Device for Stable Diffusion: cuda or cpu
SD_DEVICE=cuda
# Width of generated images
SD_IMAGE_WIDTH=512
# Height of generated images
SD_IMAGE_HEIGHT=512
# Number of inference steps for Stable Diffusion
SD_INFERENCE_STEPS=30
# Optional: LangSmith API key (leave blank to disable)
LANGSMITH_API_KEY=
# Optional: LangSmith project name
LANGSMITH_PROJECT=blog-agent